{
  "name": "housekeeping-saas-backend",
  "version": "1.0.0",
  "description": "Housekeeping SaaS System Backend",
  "scripts": {
    "typeorm": "typeorm-ts-node-commonjs -d dist/typeorm.config.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:api": "jest --testPathPattern=test/api",
    "create:admin": "ts-node -r tsconfig-paths/register src/scripts/create-admin.ts",
    "seed": "npm run create:admin",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/typeorm.config.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/typeorm.config.ts",
    "compile:scripts": "nest build --webpack --webpackPath webpack-scripts.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.17",
    "@nestjs/core": "^10.4.17",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.17",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.9.0",
    "baidu-aip-sdk": "^4.16.16",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect": "^3.7.0",
    "cos-nodejs-sdk-v5": "^2.12.1",
    "dotenv": "^16.5.0",
    "http-errors": "^2.0.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.15.6",
    "qs": "^6.14.0",
    "range-parser": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "send": "^1.2.0",
    "serve-static": "^2.2.0",
    "swagger-ui-express": "^5.0.0",
    "tencentcloud-sdk-nodejs": "^4.1.38",
    "typeorm": "^0.3.23",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "validator": "^13.15.0",
    "webidl-conversions": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@types/body-parser": "^1.19.5",
    "@types/connect": "^3.4.38",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.3.0",
    "@types/estree": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/http-errors": "^2.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/mime": "^3.0.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.46",
    "@types/qs": "^6.9.18",
    "@types/range-parser": "^1.2.7",
    "@types/semver": "^7.7.0",
    "@types/send": "^0.17.4",
    "@types/serve-static": "^1.15.7",
    "@types/validator": "^13.15.0",
    "@types/webidl-conversions": "^7.0.3",
    "@types/whatwg-url": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
