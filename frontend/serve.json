{
  "headers": [
    {
      "source": "**/*.@(js|css|html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/miniprogram/**",
      "destination": "/miniprogram/$1"
    }
  ],
  "cleanUrls": false
}

